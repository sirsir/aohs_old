		  o.attr("btnstate","show");
		  progressEnabled(true); 
		  loadTransferCall(v_id,row_no,o);
	   } else if(rowState=="show"){
		  o.attr("btnstate","hide");
		  hideTransferCall(v_id,row_no,o);	
	   } else if(rowState=="hide"){
		  o.attr("btnstate","show");
		  showTransferCall(v_id,row_no,o);			   	
	   }  
	  }
	  
